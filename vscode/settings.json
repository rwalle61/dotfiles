{
  "explorer.confirmDragAndDrop": false,
  "editor.suggest.localityBonus": true,
  "terminal.integrated.scrollback": 8000,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "zenMode.centerLayout": false,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "zenMode.restore": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": false,
    "**/.project": true,
    "**/.settings": true
  },
  "zenMode.hideLineNumbers": false,
  "editor.suggestSelection": "first",
  "files.associations": {
    "*Dockerfile*": "dockerfile"
  },
  "git.autofetch": true,
  "workbench.commandPalette.history": 30,
  "files.trimFinalNewlines": true,
  "workbench.panel.defaultLocation": "right",
  "git.ignoreLegacyWarning": true,
  "vsicons.dontShowNewVersionMessage": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
    // "source.fixAll.eslint": true
  },
  "files.insertFinalNewline": true,
  "shellcheck.disableVersionCheck": true,
  "debug.onTaskErrors": "debugAnyway",
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // "terminal.integrated.fontFamily": "Meslo LG M for Powerline",
  "editor.tabSize": 2,
  "code-runner.showRunIconInEditorTitleMenu": false,
  "gitHistory.showEditorTitleMenuBarIcons": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "editor.linkedEditing": true,
  "todohighlight.keywordsPattern": "\\sTODO:",
  "javascript.preferences.importModuleSpecifierEnding": "minimal",
  "editor.trimAutoWhitespace": false,
  "editor.renderWhitespace": "trailing",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "git.confirmSync": false,
  "workbench.iconTheme": "vscode-icons",
  "search.exclude": {
    "**/.git": true,
    "**/*.heapsnapshot": true
  },
  "errorLens.enabledDiagnosticLevels": ["info", "error"],
  "mocha-snippets.function-type": "arrow",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.validate": false,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "editor.rulers": [],
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "json.schemas": [],
  "errorLens.enabled": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "githubPullRequests.terminalLinksHandler": "github",
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "workbench.startupEditor": "none",
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "files.autoSave": "onFocusChange",
  "githubPullRequests.pullBranch": "never",
  "eslint.workingDirectories": [],
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascriptreact",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (EEx)",
    "HTML (Eex)",
    "plist"
  ],
  "workbench.editor.enablePreview": false,
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.inlayHints.callArgumentNames": "partial",
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.typeCheckingMode": "basic",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.closeDiffOnOperation": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "window.zoomLevel": 1,
  "workbench.activityBar.location": "top",
  "jupyter.askForKernelRestart": false,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "notebook.formatOnCellExecution": true,
  "notebook.formatOnSave.enabled": true,
  "notebook.experimental.variablesView": true,
  "prisma.showPrismaDataPlatformNotification": false,
  "testing.openTesting": "openExplorerOnTestStart",
  "python.testing.pytestEnabled": true,
  "testing.coverageToolbarEnabled": true
}
